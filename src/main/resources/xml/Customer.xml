<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cat.miao.dao.CustomerDao">
    <insert id="insert" parameterType="com.cat.miao.entity.Customer">
        insert into customer(name,age,birthday,birthplace) values(#{name},#{age},#{birthday},#{birthplace})
    </insert>
</mapper>